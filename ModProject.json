{
    "modName": "HKTool",
    "modVersion": "1.8.3.0",
    "codeDir": "./scripts",
    "csCompileInfo": true,
    "libraryDir": "./Library",
    "supportPreloadPrefabVersion": 70,
    "supportPreloadAssetsVersion": 72,
    "dependencies": [
        {
            "name": "Vanilla",
            "url": "https://files.catbox.moe/i4sdl6.zip",
            "ignoreFiles": [
                "Assembly-CSharp.dll",
                "mscorlib.dll",
                "Newtonsoft.Json.dll",
                "MMHOOK_Assembly-CSharp.dll"
            ],
            "copyToOutput": false
        },
        {
            "name": "Modding API",
            "url": "https://github.com/hk-modding/api/releases/latest/download/ModdingApiWin.zip",
            "ignoreFiles": [
                "MMHOOK_PlayMaker.dll",
                "Mono.Cecil.dll",
                "MonoMod.RuntimeDetour.dll",
                "MonoMod.Utils.dll",
                "MMHOOK_Assembly-CSharp.dll"
            ],
            "copyToOutput": false
        },
        {
            "name": "Satchel",
            "url": "https://github.com/PrashantMohta/Satchel/releases/latest/download/Satchel.dll"
        }
    ],
    "hktool": {
        "modifyIL": true,
        "compressResources": true,
        "referenceLib": false,
        "inlineHook": true,
        "modRes": {
            "HKTool.Languages.en.txt": {
                "name": "LANGUAGE_EN",
                "type": "utf8"
            },
            "HKTool.Languages.zh.txt": {
                "name": "LANGUAGE_ZH",
                "type": "utf8"
            }
        }
    },
    "resources": {
        "./res/Languages/en.txt": "HKTool.Languages.en.txt",
        "./res/Languages/zh-cn.txt": "HKTool.Languages.zh.txt"
    },
    "csproj": {
        "itemGroup": {
            "name": "ItemGroup",
            "content": [],
            "attr": {}
        },
        "propertyGroup": {
            "name": "PropertyGroup",
            "content": [],
            "attr": {}
        }
    },
    "enableNullableCheck": true,
    "$schema": "https://github.com/HKLab/hkmod-utils/raw/master/schemca/modProject.json"
}